<div class="method-content">
    <h3 id="{{name}}">{{name}}</h3>
    <p class="method-types"> {{{ paramSummary parameters }}}</p>
    <div class="ui divider param-divider"></div>
    <p>{{{ description }}}</p>
     {{#if parameters}}
    <table class="ui table param-table">
        <thead>
        <tr>
            <th width="20%">Parameter Name</th>
            <th width="20%">Data Type</th>
            <th width="15%">Default Value</th>
            <th width="35%">"Description</th>
        </tr>
        </thead>
        <tbody>
            {{#each parameters}}
                <tr>
                    <td width="20%">{{ name }}</td>
                    <td width="20%" class="type">{{{ typeName type }}}</td>
                    <td width="15%">{{ defaultValue }}</td>
                    <td width="35%">{{{ description }}}</td>
                </tr>
            {{/each}}
        </tbody>
        {{/if}}
    </table>
    {{#if returnParameters}}
        <table class="ui table param-table">
            <thead>
                <th width="40%">Return Type</th>
                <th width="60%">Description</th>
            </thead>
            <tbody>
                {{#each returnParameters}}
                    <tr>
                        <td class="type">{{{ typeName type }}}</td>
                        <td>{{{ description }}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    {{/if}}
</div>