<h2>New Method</h2>
<h3 >__init</h3><p>({{{ paramSummary parameters }}})</p>
<table class="ui table param-table">
    <thead>
    <tr>
        <th width="20%">Param Name</th>
        <th width="20%">Data Type</th>
        <th width="15%">Default Value</th>
        <th width="35%">Description</th>
    </tr>
    </thead>
    <tbody>
        {{#each parameters}}
        <tr>
            <td>{{ name }}</td>
            <td>{{{ typeName type }}}</td>
            <td>{{ defaultValue }}</td>
            <td>{{{ description }}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>