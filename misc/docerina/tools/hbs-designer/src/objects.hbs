<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">

  <title>API Documentation</title>
  <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.css">
  <link rel="stylesheet" href="../../styles/css/styles.css">
  <script src="https://cdn.jsdelivr.net/npm/semantic-ui@2.4.2/dist/semantic.min.js"></script>
</head>

<body>
  <div class="ui grid">
    <div class="navi-wrapper two wide column">
      <a href="../../index.html">
        <img class="logo" src="../../images/ballerina-logo.png">
      </a>
      <div class="primary-list">
        {{#if all.modules}}
        <ul>
         {{#each all.modules}}
          {{#if constructs}}
          {{#each constructs}}
          {{#if constructs.objects}}s
          {{#each objects}}
          <li>
            {{name}}
          </li>
          {{/each}}
          {{/if}}
          {{/each}}
          {{/if}}
          {{/each}}
        </ul>
        {{/if}}
      </div>...
      <div class="ui divider"></div>
      <div class="primary-list">
        <h3>{{id}}</h3>
        {{#if all.modules}}
        <ul>
          {{#each all.modules}}
          {{#if constructs}}
          {{#each constructs}}
          <li>
            <a href="../index.html#{{@key}}">{{@key}}</a>
          </li>
          {{/each}}
          {{/if}}
          {{/each}}
        </ul>
        {{/if}}
        <div class="ui divider"></div>
      </div>
       <div class="primary-list">
        <h3>All Modules</h3>
        {{#if all.modules}}
            <ul>
              {{#each all.modules}}
              <li>
                <a href="../index.html">{{id}}</a>
              </li>
              {{/each}}
            </ul>
        {{/if}}
        <div class="ui divider"></div>
      </div>
    </div>

    <div class="fourteen wide column content-wrapper">
      <h1>{{ obj.name }}</h1>
      <div class="ui divider"></div>

      <h3>{{obj.type}} <span class="objects">{{obj.name}}</span> {{obj.label}}</h3>
      <p>{{obj.description}}</p>

      <div class="constants">
        {{#if obj.method}}
          {{#each obj.method}}
          <p><b>{{method_type}}</b>{{method_params}}<b>{{label_return}}</b>{{return_params}}</p>
          {{/each}}
        {{/if}}
        {{#if all.constants}}
            {{#each constants}}
            {{#if method}}
            {{#each method}}
                <p>{{obj.method_type}} {{obj.method_params}}</p>
            {{/each}}
            {{/if}}
            {{/each}}
            {{/if}}

        <div class="ui divider"></div>
        {{#if obj.method}}
          {{#each obj.method}}
            <h3>{{method_type}}{{method_params}}</h3>
            <p>{{description}}</p>
            <table class="ui table">
              <thead>
                <tr>
                  <th>{{param_name_label}}</th>
                  <th>{{data_type_label}}</th>
                  <th>{{default_value_label}}</th>
                  <th>{{description_label}}</th>
                </tr>
              </thead>
              {{#if fields}}
                {{#each fields}}
                <tbody>
                  <tr>
                    <td>{{param_name}}</td>
                    <td>{{data_type}}</td>
                    <td>{{default_value}}</td>
                    <td>{{description}}</td>
                  </tr>
                </tbody>
                {{/each}}
                {{/if}}
              </table>
            {{/each}}
          {{/if}}
      </div>
      <div class="ui divider"></div>
    </div>
  </div>
</body>