<!DOCTYPE HTML>
<html>

<head>
    <title>{{refinePackagePath this}}</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
    <link rel="stylesheet" href="docerina-theme/css/main.css" />
    <!--[if lte IE 9]><link rel="stylesheet" href="docerina-theme/css/ie9.css" /><![endif]-->
    <!--[if lte IE 8]><link rel="stylesheet" href="docerina-theme/css/ie8.css" /><![endif]-->
</head>

<body>
    <!-- Wrapper -->
    <div id="wrapper">

        <!-- Main -->
        <div id="main">
            <div class="inner">

                <!-- Header -->
                <header id="header">
                    <a href="index.html" class="logo"><strong>Ballerina</strong> API Documentation</a>
                </header>

                <!-- Banner -->
                <section id="banner">
                    <div class="content">
                        {{#hasFunctions}}
                        <!-- Functions -->
                        <h2>Functions of {{refinePackagePath this}} package</h2>
                        {{#each functions}} {{currentObject this}}
                        <div style="padding-bottom: 30px;">
                            <h3 id="{{name}}">
                                function {{name}}({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}}) {{#if returnParameters}}({{#each returnParameters}}{{typeText typeNode}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/if}}
                            </h3>
                            <p>{{oneValueAnnotation "description"}}</p>

                            <p>
                                <b>Parameters:</b>
                                <table>
                                    <tr>
                                        <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each parameters}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                            {{#if returnParameters}}
                            <p>
                                <b>Return Parameters:</b>
                                <table>
                                    <tr>
                                        <th>Return Variable</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each returnParameters}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{paramAnnotation this "return"}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                            {{/if}}
                        </div>
                        {{/each}}
                        <p></p>
                        <br>{{/hasFunctions}} 
                        {{#hasConnectors}}
                        <!-- Connectors -->
                        <h2>Connectors of {{refinePackagePath this}} package</h2>
                        {{#each connectors}} {{currentObject this}}
                        <h3 id="{{name}}">
                            connector {{#with this}}{{this.name}} ({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/with}}
                        </h3>
                        <p>{{oneValueAnnotation "description"}}</p>
                        <p>
                            <b>Parameters:</b>
                            <table>
                                <tr>
                                    <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each parameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        <b>Actions:</b><br><br> {{#each actions}} {{currentObject this}}
                        <h3 id="{{name}}{{../name}}">action {{this.name}}({{#each parameters}}<a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a> {{name}}{{#unless @last}}, {{/unless}}{{/each}})</h3>
                        <p>{{oneValueAnnotation "description"}}</p>
                        <p>
                            <b>Parameters:</b>
                            <table>
                                <tr>
                                    <th>Parameter Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each parameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{paramAnnotation this "param"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{#if returnParameters}}
                        <p>
                            <b>Return Parameters:</b>
                            <table>
                                <tr>
                                    <th>Return Variable</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each returnParameters}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{paramAnnotation this "return"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/if}} {{/each}} {{/each}}
                        <br>{{/hasConnectors}} 
                        {{#hasStructs}}
                        <!-- Structs -->
                        <h2>Structs of {{refinePackagePath this}} package</h2>
                        {{#each structs}} {{currentObject this}}
                        <h3 id="{{name}}">struct {{name}}</h3>
                        <p>
                            <b>Fields:</b>
                            <table>
                                <tr>
                                    <th>Field Name</th><th>Data Type</th><th>Description</th>
                                </tr>
                                {{#each fields}}
                                <tr>
                                    <td>{{name}}</td><td><a href="{{#bindLink typeKind}}{{/bindLink}}"{{typeTitle typeKind}}>{{typeText typeNode}}</a></td><td>{{fieldAnnotation this "field"}}</td>
                                </tr>
                                {{/each}}
                            </table>
                        </p>
                        {{/each}}<br> {{/hasStructs}}
                        {{#hasAnnotations}}
                        <!-- Annotations -->
                        <h2>Annotations of {{refinePackagePath this}} package</h2>
                        {{#each annotations}} {{currentObject this}}
                        <div style="padding-bottom: 30px;">
                            <h3 id="{{name}}">annotation {{name}}</h3>
                            <p>{{oneValueAnnotation "description"}}</p>

                            <p>
                                <b>Attributes:</b>
                                <table>
                                    <tr>
                                        <th>Attribute Name</th><th>Data Type</th><th>Description</th>
                                    </tr>
                                    {{#each attributes}}
                                    <tr>
                                        <td>{{name}}</td><td><a href="{{#bindLink typeNode}}{{/bindLink}}"{{typeTitle typeNode}}>{{typeText typeNode}}</a></td><td>{{attributeAnnotation this "attributeDef"}}</td>
                                    </tr>
                                    {{/each}}
                                </table>
                            </p>
                        </div>
                        {{/each}}
                        <p></p>
                        <br>{{/hasAnnotations}} 
                    </div>
                </section>
            </div>
        </div>

        <!-- Sidebar -->
        <div id="sidebar">
            <div class="inner">

                <!-- Menu -->
                <nav id="menu" style="overflow-x: auto">
                    <header class="major">
                        <h2>Menu</h2>
                    </header>
                    <ul>
                        {{#hasFunctions}}
                        <li>
                            <span class="opener">Functions</span>
                            <ul>
                            {{#each functions}}
                                <li><a href="#{{name}}">{{name}}({{#each parameters}}{{typeText typeNode}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}) {{#if returnParameters}}({{#each returnParameters}}{{typeText typeNode}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/if}}</a>
                                </li>{{/each}}
                            </ul>
                        </li>{{/hasFunctions}} {{#hasConnectors}}
                        <li>
                            <span class="opener">Connectors</span>
                            <ul>
                                {{#each connectors}}
                                <li><a href="#{{this.name}} ">{{#with this}}{{this.name}} ({{#each parameters}}{{typeText typeNode}} {{name}}{{#unless @last}}, {{/unless}}{{/each}}){{/with}}</a>
                                    <ul>
                                        {{#each actions}}
                                        <li><a href="#{{name}}{{../name}}">{{this.name}}({{#each parameters}}{{#if @first}}{{else}}{{typeText typeNode}} {{name}}{{#unless @last}}, {{/unless}}{{/if}}{{/each}})</a></li>
                                        {{/each}}
                                    </ul>
                                </li>{{/each}}
                            </ul>
                        </li>{{/hasConnectors}} {{#hasStructs}}
                        <li>
                            <span class="opener">Structs</span>
                            <ul>
                                {{#each structs}}
                                    <li><a href="#{{name}} ">{{name}}</a></li>
                                {{/each}}
                            </ul>
                        </li>{{/hasStructs}} {{#hasAnnotations}}
                        <li>
                            <span class="opener">Annotations</span>
                            <ul>
                                {{#each annotations}}
                                    <li><a href="#{{name}} ">{{name}}</a></li>
                                {{/each}}
                            </ul>
                        </li>{{/hasAnnotations}}
                    </ul>
                </nav>

                <!-- Footer -->
                <footer id="footer">
                    <p class="copyright">Copyright 2017 Ballerina API Documentation</p>
                </footer>
            </div>
        </div>
    </div>

    <!-- Scripts -->
    <script src="docerina-theme/js/jquery.min.js"></script>
    <script src="docerina-theme/js/skel.min.js"></script>
    <script src="docerina-theme/js/util.js"></script>
    <!--[if lte IE 8]><script src="docerina-theme/js/ie/respond.min.js"></script><![endif]-->
    <script src="docerina-theme/js/main.js"></script>
</body>
</html>
